{
  "name": "@modernpoacher/zashiki-react-redux",
  "version": "2.6.14",
  "description": "React + Redux for Zashiki",
  "keywords": [
    "React",
    "Redux",
    "React Redux",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-react-redux.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel -- --ignore \"**/__tests__/**\"",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib"
  },
  "dependencies": {
    "@modernpoacher/isomorphic": "0.0.252",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3",
    "prop-types": "^15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^8.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "shinkansen-engine": "1.0.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-export-default-from": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.24.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "rimraf": "^3.0.2"
