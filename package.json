{
  "name": "@modernpoacher/zashiki-react-redux",
  "version": "2.0.2",
  "description": "React + Redux for Zashiki",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-react-redux.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run babel",
    "build:clean": "npm run clean && cross-env NODE_ENV=production npm run build",
    "clean": "bash clean.sh",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint *.js src config",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run babel && git add lib",
    "deps": "deps",
    "deps:prod": "deps --save-prod",
    "deps:dev": "deps --save-dev"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "UNLICENSED",
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "@modernpoacher/isomorphic": "0.0.2",
    "axios": "^0.19.0",
    "cross-env": "^5.2.1",
    "immutable": "3.8.2",
    "mock-browser": "^0.92.14",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "shinkansen-gears": "1.0.0",
    "shinkansen-motor": "1.0.0",
    "shinkansen-pantograph": "1.0.0",
    "shinkansen-rails": "1.0.0",
    "shinkansen-signals": "1.0.0",
    "zashiki-promise-middleware": "^1.0.10",
    "zashiki-routing-middleware": "^1.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@modernpoacher/deps": "0.0.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "core-js": "^3.2.1",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
