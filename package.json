{
  "name": "@modernpoacher/zashiki-react-redux",
  "version": "2.6.14",
  "description": "React + Redux for Zashiki",
  "keywords": [
    "React",
    "Redux",
    "React Redux",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-react-redux.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel -- --ignore \"**/__tests__/**\"",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib"
  },
  "dependencies": {
    "@modernpoacher/isomorphic": "0.0.21",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "fast-deep-equal": "^3.1.3",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^7.2.6",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "shinkansen-engine": "1.0.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-export-default-from": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.16.5",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.20.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2"
  }
