{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
